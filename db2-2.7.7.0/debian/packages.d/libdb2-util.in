%files%
usr/bin
%preinst%
#!/bin/sh
# the version of libdb2-util distributed with potato had a symlink here.
# due to dpkg's design, replacing it with a directory will go badly wrong
# if we don't remove the symlink first.
if [ -L /usr/share/doc/libdb2-util ]; then
	rm -f /usr/share/doc/libdb2-util
fi

exit 0

#DEBHELPER#
