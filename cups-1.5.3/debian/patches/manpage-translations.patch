+#: lp.man:185
+msgid "Scales the print file to fit on the page."
+msgstr "Mettre le fichier √† l'√©chelle pour qu'il prenne la taille de la page."
+
+# type: TP
+#. type: TP
+#: lp.man:185
+#, no-wrap
+msgid "-o number-up=2"
+msgstr "-o number-up=2"
+
+# type: TP
+#. type: TP
+#: lp.man:187
+#, no-wrap
+msgid "-o number-up=4"
+msgstr "-o number-up=4"
+
+# type: TP
+#. type: TP
+#: lp.man:189
+#, no-wrap
+msgid "-o number-up=6"
+msgstr "-o number-up=6"
+
+# type: TP
+#. type: TP
+#: lp.man:191
+#, no-wrap
+msgid "-o number-up=9"
+msgstr "-o number-up=9"
+
+# type: TP
+#. type: TP
+#: lp.man:193
+#, no-wrap
+msgid "-o number-up=16"
+msgstr "-o number-up=16"
+
+#. type: Plain text
+#: lp.man:197
+msgid "Prints multiple document pages on each output page."
