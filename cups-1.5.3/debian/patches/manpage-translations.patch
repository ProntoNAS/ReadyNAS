+
+# type: Plain text
+#. type: Plain text
+#: lpoptions.man.in:40
+msgid ""
+"B<lpoptions> [ -E ] [ -U I<username> ] [ -h I<server[:port]> ] [ -p "
+"I<destination[/instance]> ] -l"
+msgstr ""
+"B<lpoptions> [ -E ] [ -U I<utilisateur> ] [ -h I<serveur[:port]> ] [ -p "
+"I<destination[/instance]> ] -l"
+
+# type: Plain text
+#. type: Plain text
+#: lpoptions.man.in:54
+msgid ""
