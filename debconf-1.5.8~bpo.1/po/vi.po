msgstr "Không thể khởi chạy lối vào : %s"

#: ../Debconf/Config.pm:130
msgid "Config database not specified in config file."
msgstr "Cơ sở dữ liệu cấu hình không được ghi rõ trong tập tin cấu hình."

#: ../Debconf/Config.pm:134
msgid "Template database not specified in config file."
msgstr "Cơ sở dữ liệu mẫu không được ghi rõ trong tập tin cấu hình."

#: ../Debconf/Config.pm:139
msgid ""
"The Sigils and Smileys options in the config file are no longer used. Please "
"remove them."
msgstr "Không còn dùng lại những tùy chọn Sigils và Smileys. Hãy gỡ bỏ chúng."

#: ../Debconf/Config.pm:153
#, perl-format
msgid "Problem setting up the database defined by stanza %s of %s."
msgstr ""
"Gặp lỗi khi thiết lập cơ sở dữ liệu được xác định bởi đoạn dòng %s của %s."

#: ../Debconf/Config.pm:228
msgid ""
"  -f,  --frontend\t\tSpecify debconf frontend to use.\n"
"  -p,  --priority\t\tSpecify minimum priority question to show.\n"
"       --terse\t\t\tEnable terse mode.\n"
msgstr ""
"  -f,  --frontend\t\tGhi rõ _lối vào_ debconf cần dùng.\n"
"  -p,  --priority\t\tSGhi rõ câu hỏi _ưu tiên_ tối thiểu cân hiển thị.\n"
"       --terse\t\t\tBật chế độ _ngắn gọn_.\n"

#: ../Debconf/Config.pm:308
#, perl-format
msgid "Ignoring invalid priority \"%s\""
msgstr "Đang bỏ qua ưu tiên không hợp lê « %s »"

#: ../Debconf/Config.pm:309
#, perl-format
msgid "Valid priorities are: %s"
msgstr "Ưu tiên hợp lệ: %s"

