# Network UPS Tools: data

prefix		= @prefix@
exec_prefix	= @exec_prefix@

DATADIR		= $(DESTDIR)@datadir@

INSTALLCMD  = @INSTALL@
INSTALLMODE = 0644

DATA_FILES  = cmdvartab driver.list

all:

clean:

install:
	@if (test ! -d $(DATADIR)) then \
		echo ../install-sh -d $(DATADIR) || exit 1;\
		../install-sh -d $(DATADIR) || exit 1;\
	fi
	@for f in $(DATA_FILES) ; do \
		echo $(INSTALLCMD) -m $(INSTALLMODE) $$f $(DATADIR); \
		$(INSTALLCMD) -m $(INSTALLMODE) $$f $(DATADIR); \
	done
