#!/bin/sh -e
# $Id: krb5-telnetd.postinst,v 1.1 2000/10/26 01:31:46 hartmans Exp $

if [ "$1" = configure ] && [ -z "$2" ]; then
        update-inetd --group Kerberos --add 'telnet\t\tstream\ttcp\tnowait\troot\t/usr/sbin/tcpd\t/usr/sbin/telnetd -a user'
else
        update-inetd --enable telnet
fi

