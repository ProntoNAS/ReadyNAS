#!/bin/sh -e

if [ "$1" = "purge" ]; then
	rm -f /etc/pam.d/common-auth /etc/pam.d/common-account \
	      /etc/pam.d/common-session /etc/pam.d/common-password
fi

case $1 in
    abort-upgrade|abort-install)
        mv /etc/pam.d/other.pre-upgrade /etc/pam.d/other 2>/dev/null ||true
	;;
	esac
	


