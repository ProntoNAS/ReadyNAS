	rm -rf BUILD RPMS SPECS SOURCES SRPMS

@PACKAGE@.spec: package.spec
	cp $< $@

rpm: Makefile package.spec
@RPM_LIBUSB_DEVEL_TRUE@	@echo "Building libgphoto2 RPM with USB support and dependency on the libusb RPM package."
@RPM_LIBUSB_DEVEL_FALSE@	@echo "#############################################################################"
@RPM_LIBUSB_DEVEL_FALSE@	@echo "# Building libgphoto2 RPM packages ***WITHOUT*** USB support.               #"
@RPM_LIBUSB_DEVEL_FALSE@	@echo "# Install both the libusb and the libusb-devel RPM package for USB support. #"
@RPM_LIBUSB_DEVEL_FALSE@	@echo "#############################################################################"
@RPM_LIBUSB_DEVEL_FALSE@	@sleep 3
	$(MAKE) clean
	mkdir -p BUILD SPECS SOURCES SRPMS
	for arch in i386 i486 i586 i686 alpha noarch; do \
		mkdir -p "RPMS/$${arch}"; \
	done
	$(MAKE) SOURCES/@PACKAGE@-@VERSION@.tar.gz SPECS/@PACKAGE@-@VERSION@.spec
	@RPMBUILD@ --define "_topdir $$(pwd)" -ba SPECS/@PACKAGE@-@VERSION@.spec

SOURCES/@PACKAGE@-@VERSION@.tar.gz: $(top_srcdir)/@PACKAGE@-@VERSION@.tar.gz
	ln -f $< $@

SPECS/@PACKAGE@-@VERSION@.spec: package.spec
	ln -f $< $@
