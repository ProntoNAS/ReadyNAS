#: ../adduser:101
msgid "No options allowed after names.\n"
msgstr "L'utilisation d'options n'est pas permise après les noms.\n"

#: ../adduser:118
msgid "--ingroup requires an argument.\n"
msgstr "--ingroup nécessite un argument.\n"

#: ../adduser:121
msgid "--home requires an argument.\n"
msgstr "--home nécessite un argument.\n"

#: ../adduser:123
msgid "Warning: The home dir you specified already exists.\n"
msgstr "Attention: le répertoire personnel que vous avez spécifié existe déjà.\n"

#: ../adduser:125
msgid "The home dir must be an absolute path.\n"
msgstr "Le répertoire personnel doit être indiqué par un chemin absolu.\n"

#: ../adduser:132
msgid "--uid requires a numeric argument.\n"
msgstr "--uid nécessite un argument numérique.\n"

#: ../adduser:135
msgid "--gid requires a numeric argument.\n"
msgstr "--gid nécessite un argument numérique.\n"

#: ../adduser:138
msgid "--conf requires an argument.\n"
msgstr "--conf nécessite un argument.\n"

#: ../adduser:140
#, c-format
msgid "`%s' doesn't exist.\n"
msgstr "« %s » n'existe pas.\n"

#: ../adduser:147
#, c-format
msgid "Unknown argument `%s'.\n"
msgstr "Argument « %s » inconnu.\n"

#: ../adduser:153
msgid "Only root may add a user or group to the system.\n"
msgstr "Seul root peut ajouter un utilisateur ou un groupe sur le système.\n"

#: ../adduser:156
msgid "Enter a username to add: "
msgstr "Veuillez indiquer le nom d'utilisateur à ajouter : "

#: ../adduser:160
msgid "I need a name to add.\n"
msgstr "J'ai besoin d'un nom à ajouter.\n"

#: ../adduser:161
msgid "No more than two names.\n"
msgstr "Pas plus de deux noms.\n"

#: ../adduser:163
msgid "Specify only one name in this mode.\n"
msgstr "Ne spécifiez qu'un nom dans ce mode.\n"

#: ../adduser:181
msgid "--group, --ingroup, and --gid options are mutually exclusive.\n"
msgstr "Les options --group, --ingroup et --gid s'excluent mutuellement.\n"

#: ../adduser:214
#, c-format
msgid "The group `%s' already exists.\n"
msgstr "Le groupe « %s » existe déjà.\n"

#: ../adduser:216
#, c-format
msgid "The GID `%s' is already in use.\n"
msgstr "Le GID « %s » est déjà utilisé.\n"

#: ../adduser:224
msgid "No GID is available in the range "
msgstr "Aucun GID n'est disponible dans l'interval "

#: ../adduser:227
#, c-format
msgid "Group `%s' not created.\n"
msgstr "Pas de création du groupe `%s'.\n"

#: ../adduser:231
#, c-format
msgid "Adding group %s (%s)...\n"
msgstr "Ajout du groupe %s (%s)...\n"

#: ../adduser:233 ../adduser:257
msgid "Done.\n"
msgstr "Fait.\n"

#: ../adduser:242
#, c-format
msgid "The user `%s' doesn't exist.\n"
msgstr "L'utilisateur « %s » n'existe pas.\n"

#: ../adduser:244
#, c-format
msgid "The group `%s' doesn't exist.\n"
msgstr "Le groupe « %s » n'existe pas.\n"

#: ../adduser:247
#, c-format
msgid "The user `%s' is already a member of %s.\n"
msgstr "L'utilisateur « %s » est déjà membre du groupe %s.\n"

#: ../adduser:252
#, c-format
msgid "Adding user %s to group %s...\n"
msgstr "Ajout de l'utilisateur %s au groupe %s...\n"

#: ../adduser:269
#, c-format
msgid "Adding system user %s...\n"
msgstr "Ajout de l'utilisateur système %s...\n"

#: ../adduser:276 ../adduser:354
msgid "No UID/GID pair is available in the range "
msgstr "Aucune paire UID/GID n'est disponible dans l'interval "

#: ../adduser:279 ../adduser:292 ../adduser:357 ../adduser:370
#, c-format
msgid "User `%s' not created.\n"
msgstr "L'utilisateur « %s » n'a pas été créé.\n"

#: ../adduser:289 ../adduser:367
msgid "No UID is available in the range "
msgstr "Aucun UID n'est disponible dans l'interval "

#: ../adduser:296 ../adduser:302 ../adduser:374 ../adduser:380
msgid "Internal error"
msgstr "Erreur interne"

#: ../adduser:306
msgid "Adding new group $new_name ($new_gid).\n"
msgstr "Ajout du nouveau groupe $new_name ($new_gid).\n"

#: ../adduser:311 ../adduser:389
#, c-format
msgid "Adding new user %s (%s) with group %s.\n"
msgstr "Ajout du nouvel utilisateur %s (%s) avec comme groupe « %s ».\n"

#: ../adduser:320
#, c-format
msgid "Home directory %s already exists.\n"
msgstr "Le répertoire personnel %s existe déjà.\n"

#: ../adduser:322
msgid "Not creating home directory.\n"
msgstr "Pas de création de répertoire personnel.\n"

#: ../adduser:324 ../adduser:403
#, c-format
msgid "Creating home directory %s.\n"
msgstr "Création du répertoire personnel %s.\n"

#: ../adduser:347
#, c-format
msgid "Adding user %s...\n"
msgstr "Ajout de l'utilisateur %s...\n"

#: ../adduser:384
#, c-format
msgid "Adding new group %s (%s).\n"
msgstr "Ajout du nouveau groupe %s (%s).\n"

#: ../adduser:397
#, c-format
msgid "Home directory %s already exists.  Not copying from %s\n"
msgstr "Le répertoire personnel %s existe déjà.  Pas de recopie de %s\n"

#: ../adduser:413
#, c-format
msgid "Copying files from %s\n"
msgstr "Copie des fichiers depuis %s\n"

#: ../adduser:435
msgid "Is the information correct? [y/n] "
msgstr "Ces informations sont-elles correctes ? [y/n] "

#: ../adduser:442
#, c-format
msgid "Setting quota from %s.\n"
msgstr "Mise en place du quota à partir de %s.\n"

