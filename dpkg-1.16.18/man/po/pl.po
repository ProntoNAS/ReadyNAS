#~ "  DEB_HOST_ARCH_CPU := $(shell dpkg-architecture -qDEB_HOST_GNU_CPU)\n"
#~ "  ifeq ($(DEB_HOST_ARCH_CPU),x86_64)\n"
#~ "    DEB_HOST_ARCH_CPU := amd64\n"
#~ "  endif\n"
#~ "endif\n"
#~ "ifeq ($(DEB_HOST_ARCH_OS),)\n"
#~ "  DEB_HOST_ARCH_OS := $(subst -gnu,,$(shell dpkg-architecture -"
