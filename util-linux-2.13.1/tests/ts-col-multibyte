. ./commands.sh
. ./functions.sh

TS_COMPONENT="col"
TS_DESC="multibyte"

ts_init "$*"

cat $TS_INPUT | $TS_CMD_COL > /dev/null  2> $TS_OUTPUT

ts_finalize

