#!/bin/sh
set -e

cd "$ADTTMP"
cat > test.c << EOF
#include <libraw/libraw.h>
#include <iostream>
using namespace std;

int main(void)
{
        LibRaw RawProcessor;
        cout << LibRaw::version() << endl;
        return 0;
}

EOF
g++ -Wall -o test test.c -lraw
./test
objdump -p test | grep "libraw"
