# udev rules for the NUT USB drivers

SUBSYSTEM!="usb_device", GOTO="nut-usbups_rules_end"
ACTION!="add", GOTO="nut-usbups_rules_end"

# MGE UPS SYSTEMS - usbhid-ups
SYSFS{idVendor}=="0463", SYSFS{idProduct}=="ffff", MODE="664", GROUP="admin"
SYSFS{idVendor}=="0463", SYSFS{idProduct}=="0001", MODE="664", GROUP="admin"

# APC - usbhid-ups
SYSFS{idVendor}=="051d", SYSFS{idProduct}=="0002", MODE="664", GROUP="admin"

# CyberPower - usbhid-ups
SYSFS{idVendor}=="0764", SYSFS{idProduct}=="0005", MODE="664", GROUP="admin"
SYSFS{idVendor}=="0764", SYSFS{idProduct}=="0501", MODE="664", GROUP="admin"

# Powercom - usbhid-ups
SYSFS{idVendor}=="0d9f", SYSFS{idProduct}=="0001", MODE="664", GROUP="admin"

# Powerware - bcmxcp_usb
SYSFS{idVendor}=="0592", SYSFS{idProduct}=="0002", MODE="664", GROUP="admin"

# Tripp Lite - tripplite_usb
SYSFS{idVendor}=="09ae", SYSFS{idProduct}=="0001", MODE="664", GROUP="admin"

# Tripp Lite - usbhid-ups
SYSFS{idVendor}=="09ae", SYSFS{idProduct}=="2005", MODE="664", GROUP="admin"
SYSFS{idVendor}=="09ae", SYSFS{idProduct}=="1003", MODE="664", GROUP="admin"

# Belkin - usbhid-ups
SYSFS{idVendor}=="050d", SYSFS{idProduct}=="0980", MODE="664", GROUP="admin"
SYSFS{idVendor}=="050d", SYSFS{idProduct}=="0900", MODE="664", GROUP="admin"
SYSFS{idVendor}=="050d", SYSFS{idProduct}=="0912", MODE="664", GROUP="admin"
SYSFS{idVendor}=="050d", SYSFS{idProduct}=="0551", MODE="664", GROUP="admin"
SYSFS{idVendor}=="050d", SYSFS{idProduct}=="0751", MODE="664", GROUP="admin"

# Kebo - unsupported
SYSFS{idVendor}=="0925", SYSFS{idProduct}=="1234", MODE="664", GROUP="admin"

LABEL="nut-usbups_rules_end"

