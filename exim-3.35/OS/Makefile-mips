LIBS=-liberty
XINCLUDE=-I/usr/X11R6/include

CFLAGS=-O
PCRE_CFLAGS=-DUSE_BCOPY -DSTRERROR_FROM_ERRLIST

EXIWHAT_PS_ARG=-ax
EXIWHAT_EGREP_ARG='/exim( |$$)'
EXIWHAT_KILL_ARG=-30

# End
