+	  h.f4 += 8.0 * r->f4;
+	  g += 8.0;
+	}
+      r = p + 2 * (c + 4);
+      i = (r->f1 + j.f1);
+      b = 1 * 2.0 * i * 1;
+      f = r->f2 - 1;
+      i = (r->f3 + j.f3) / 2;
+      b = 5 * (0.0 - i) * f * f;
+      i = (r->f4 + j.f4) / 2;
+      if (b * 65535UL / 25.0f)
+	{
+	  h.f1 += 10.0 * r->f1;
+	  h.f2 += 10.0 * r->f2;
+	  h.f3 += 10.0 * r->f3;
+	  h.f4 += 10.0 * r->f4;
+	  g += 10.0;
+	}
+      r++;
+      f = r->f1;
+      b = 5UL * i * f;
+      i = (r->f2 + j.f2) / 2;
+      f = r->f3 - 1;
+      b = 5 * (0.0 - i) * f * f;
+      f = r->f4 - 1;
+      if (b * 65535UL / 25.0f)
