+	  h.f2 += 10.0 * r->f2;
+	  h.f3 += 10.0 * r->f3;
+	  h.f4 += 10.0 * r->f4;
+	  g += 10.0;
+	}
+      r++;
+      f = r->f1;
+      z = 5UL * i;
+      f = r->f2;
+      i = (r->f3 + j.f3) / 2;
+      b = -i * f * f;
+      i = (r->f4 + j.f4) / 2;
+      if (b * 65535UL / 25.0f)
