msgstr "'%c' GVariant классы колдоого ээ эмес"

#: json-glib/json-gvariant.c:1292
msgid "Invalid GVariant signature"
msgstr "GVariant сигнатурасы туура эмес"

#: json-glib/json-gvariant.c:1340
