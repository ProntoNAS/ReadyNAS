# subversion completion by Jean-Baptiste Quenot <jb.quenot@caraldi.com>
#
# $Id: subversion,v 1.1 2004/03/30 23:12:00 ianmacd Exp $

_svn()
{
	local cur

	COMPREPLY=()
	cur=${COMP_WORDS[COMP_CWORD]}

	[ $COMP_CWORD = 1 ] && \
	COMPREPLY=( $( compgen -W 'add cat checkout cleanup commit copy \
				   delete diff export help import info list \
				   log merge mkdir move propdel propedit \
				   propget proplist propset resolved revert \
				   status switch update' -- $cur ) )

	return 0
}
complete -F _svn -o default svn
